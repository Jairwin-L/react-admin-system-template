{
  "name": "react-admin-system-template",
  "version": "0.0.0",
  "homepage": "https://github.com/Jairwin-L/admin-system-template#readme",
  "bugs": {
    "url": "https://github.com/Jairwin-L/admin-system-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jairwin-L/admin-system-template.git"
  },
  "funding": "https://github.com/sponsors/Jairwin-L",
  "author": {
    "name": "Jairwin",
    "email": "jairwin@jairwin.cn"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:mock": "vite --mode mock",
    "dev:prod": "vite --mode production",
    "dev:pre": "vite --mode pre",
    "build:dev": "vite build --mode development",
    "build:pre": "tsc && vite build --mode pre",
    "build:prod": "vite build",
    "lint": "eslint . && stylelint \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --fix \"**/*.{css,less,scss}\"",
    "prepare": "husky install",
    "preview": "vite preview",
    "typecheck": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/icons": "^4.8.3",
    "@types/lodash-es": "^4.17.12",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "antd": "^5.22.2",
    "china-division": "^2.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "flyio": "^0.6.14",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-image-crop": "^11.0.7",
    "react-router": "^7.0.1",
    "terser": "^5.30.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.79",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.1",
    "commitlint-config-ali": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-ali": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-config-ali": "^1.3.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.3.1",
    "stylelint-config-ali": "^2.1.1",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.5.11",
    "vite": "^6.0.1",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-externals": "^0.1.7",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "commitlint": {
    "extends": [
      "ali"
    ]
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "eslintConfig": {
    "extends": [
      "ali/typescript/react",
      "plugin:prettier/recommended"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "ci": {
    "type": "ci",
    "stages": {
      "lint": [
        "lint",
        "typecheck"
      ]
    },
    "coverage": false
  },
  "cnpm": {
    "mode": "pnpm"
  },
  "npm": {
    "mode": "pnpm"
  },
  "yarn": {
    "mode": "pnpm"
  }
}
